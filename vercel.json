{
    "version": 2,
    "builds": [
      {
        "src": "src/vite.config.js",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/weather(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/api/search(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/dist/$1"
      }
    ]
  }